<?xml version="1.0" encoding="utf-8"?>
<configuration>
  

  
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
    <section name="oracle.manageddataaccess.client"
      type="OracleInternal.Common.ODPMSectionHandler, Oracle.ManagedDataAccess, Version=4.122.1.0, Culture=neutral, PublicKeyToken=89b483f429c47342"/>
  </configSections>
  
  <log4net>
    <root>
      <level value="ALL"/>
      <appender-ref ref="RollingFileAppender"/>
    </root>
  
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="CheckIntegrationLog.txt"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="5"/>
      <maximumFileSize value="1MB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newline"/>
      </layout>
    </appender>
   </log4net>

  <appSettings>
    <add key="ProcessedFolder" value="C:\temp\Processed"/>
    <add key="SuccessfulFolder" value="C:\temp\Processed\OK"/>
    <add key="RegexFileName" value="(IAS).*(\.txt)"/>
    <add key="dbUser" value="marcel"/>
    <add key="dbPass" value="kanekomarcel"/>
  
  
  </appSettings>
  
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup>
<system.data>
<DbProviderFactories>
<remove invariant="Oracle.ManagedDataAccess.Client"/>
<add name="ODP.NET, Managed Driver" invariant="Oracle.ManagedDataAccess.Client" description="Oracle Data Provider for .NET, Managed Driver"
type="Oracle.ManagedDataAccess.Client.OracleClientFactory, Oracle.ManagedDataAccess, Version=4.122.1.0, Culture=neutral, PublicKeyToken=89b483f429c47342"/>
</DbProviderFactories>
</system.data>
<runtime>
<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
<dependentAssembly>
<publisherPolicy apply="no"/>
<assemblyIdentity name="Oracle.ManagedDataAccess" publicKeyToken="89b483f429c47342" culture="neutral"/>
<bindingRedirect oldVersion="4.121.0.0 - 4.65535.65535.65535" newVersion="4.122.1.0"/>
</dependentAssembly>
</assemblyBinding>
</runtime>
<oracle.manageddataaccess.client>
<version number="*">
<dataSources>
<dataSource alias="mytestDB" descriptor="(DESCRIPTION=(ADDRESS=(PROTOCOL=tcp)(HOST=mytestoracle.crkiu0wjjywx.sa-east-1.rds.amazonaws.com)(PORT=1521))(CONNECT_DATA=(SID=ORCL))) "/>
</dataSources>
</version>
</oracle.manageddataaccess.client>
</configuration>
